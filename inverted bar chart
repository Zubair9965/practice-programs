import java.util.*;
import java.io.*;
public class Main{
public static void main(String[]args){
Scanner scn=new Scanner(System.in);
int n=scn.nextInt();
int []arr=new int[n];
for(int i=0;i<arr.length;i++){
arr[i]=scn.nextInt();
}
int count=0;
for(int i=0;i<arr.length;i++){
if(arr[i]==count){
count++;
}
}
while(n!=arr.length){
for(int i=0;i<arr.length;i++){
if(arr[i]==0){
System.out.print("\t");
}else{
System.out.print("*\t");
arr[i]--;
if(arr[i]==0){
count++;
}
}
}
System.out.println();
}
}
}
